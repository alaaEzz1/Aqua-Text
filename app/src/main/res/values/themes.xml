<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AquaText" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/home_cardview_color</item>
        <!-- background to up Action Bar -->
        <item name="colorPrimaryVariant">@color/home_cardview_color</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color, progressbar circular color -->
        <item name="colorSecondary">@color/home_cardview_color</item>
        <item name="colorSecondaryVariant">@color/main_dark_bg</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>