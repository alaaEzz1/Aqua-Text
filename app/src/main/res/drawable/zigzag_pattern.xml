<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Zigzag pattern layer -->
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:color="@color/white"
                android:width="2dp"
                android:dashGap="4dp"
                android:dashWidth="4dp" />
        </shape>
    </item>
</layer-list>
